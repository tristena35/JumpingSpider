<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.aguilartristen.jumpingspider">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/spider_man_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/spider_man_app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".StartGameActivity">
        </activity>
        <activity android:name=".GameOverActivity">
        </activity>
        <activity android:name=".GameView">
        </activity>
        <activity android:name=".OptionsActivity">
        </activity>
        <activity android:name=".PopUpWindow" android:theme="@style/AppTheme.CustomTheme">
        </activity>
        <activity android:name=".PauseMenu" android:theme="@style/AppTheme.CustomTheme">
        </activity>

    </application>

</manifest>